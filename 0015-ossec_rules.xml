    <mitre>
      <id>T1014</id>
    </mitre>
    <group>rootcheck,</group>
  </rule>

  <!-- Process monitoring rules -->
  <rule id="530" level="0">
    <if_sid>500</if_sid>
    <match>^ossec: output: </match>
    <description>OSSEC 进程监控规则</description>
    <group>process_monitor,</group>
  </rule>

  <rule id="531" level="7" ignore="7200">
    <if_sid>530</if_sid>
    <match>ossec: output: 'df -P': /dev/</match>
    <regex>100%</regex>
    <description>分区使用率达到100% (磁盘空间监控).</description>
    <group>low_diskspace,pci_dss_10.6.1,gpg13_10.1,gdpr_IV_35.7.d,hipaa_164.312.b,nist_800_53_AU.6,tsc_CC7.2,tsc_CC7.3,</group>
  </rule>

 <rule id="532" level="0">
    <if_sid>531</if_sid>
    <match>cdrom|/media|usb|/mount|floppy|dvd</match>
    <description>忽略外部媒体</description>
  </rule>

  <rule id="533" level="7">
    <if_sid>530</if_sid>
    <match>ossec: output: 'netstat listening ports</match>
    <check_diff />
    <description>受监听的端口状态 (netstat) 改变 (开启或关闭新端口).</description>
    <group>pci_dss_10.2.7,pci_dss_10.6.1,gpg13_10.1,gdpr_IV_35.7.d,hipaa_164.312.b,nist_800_53_AU.14,nist_800_53_AU.6,tsc_CC6.8,tsc_CC7.2,tsc_CC7.3,</group>
  </rule>

  <rule id="534" level="1">
    <if_sid>530</if_sid>
    <match>ossec: output: 'w'</match>
    <check_diff />
    <options>no_log</options>
    <description>已登录用户列表. 默认不会告警.</description>
  </rule>

  <rule id="535" level="1">
    <if_sid>530</if_sid>
    <match>ossec: output: 'last -n </match>
    <check_diff />
    <options>no_log</options>
    <description>最近登录用户列表</description>
    <group>pci_dss_10.6.1,hipaa_164.312.b,</group>
  </rule>

  <rule id="536" level="0">
    <if_sid>531</if_sid>
      <regex>'df -P':\s+/dev/loop\d+\s+\d+\s+\d+\s+0\s+100%\s+/snap/\w+/\d+</regex>
      <description>忽略快照磁盘，因为它们的容量始终处于100%</description>
  </rule>


